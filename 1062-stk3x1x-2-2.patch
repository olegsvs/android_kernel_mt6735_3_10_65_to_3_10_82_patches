+	}
+	
+	if(*status & STK_FLG_PSINT_MASK)
+	{
+		set_bit(STK_BIT_PS,  &obj->pending_intr);
+	}
+	else
+	{
+	    clear_bit(STK_BIT_PS, &obj->pending_intr);
+	}
+	
+	if(atomic_read(&obj->trace) & STK_TRC_DEBUG)
+	{

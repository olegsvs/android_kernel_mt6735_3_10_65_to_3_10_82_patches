+	{
+		APS_DBG("error: %d\n", err);
+		return -EFAULT;
+	}
+	*ges0 = (buf[0]<<8) | buf[1];	
+	*ges1 = (buf[2]<<8) | buf[3];	
+	//APS_LOG( "%s: ges=%d,%d\n",__func__, *ges0, *ges1);	
+	return 0;

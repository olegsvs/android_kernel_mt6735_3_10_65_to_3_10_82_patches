-	switch (cmd)
-	{
-		case ALSPS_SET_PS_MODE:
-			if(copy_from_user(&enable, ptr, sizeof(enable)))
-			{
-				err = -EFAULT;
-				goto err_out;
-			}
-			if(enable)
-			{
-				if((err = stk3x1x_enable_ps(obj->client, 1)))

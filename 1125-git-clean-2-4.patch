-        APS_LOG("setting %d, gain %x, als %d\n", setting, setting>>6,  ch1);
-        APS_LOG("skip FF in auto gain\n\n");
-    }
-    else
-    {
-        switch (setting>>6)
-        {
-            case EPL_H_GAIN:
-                ch1 = ch1  >> 6;
-                break;
-
-            case EPL_M_GAIN:
-                ch1 = ch1  >> 3;
-                break;
-        }
-
-        *data =  ch1;
-        APS_LOG("read als raw data = %d\n",ch1);
-    }
-
-    return 0;

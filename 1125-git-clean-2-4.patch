-{
-    int i, j,k, channel_value;
-    int idx = start_idx+ges_count;
-
-    if(idx>=BUF_SIZE)
-        idx-=BUF_SIZE;
-
-
-    if(ir_on>0)
-    {
-        for(i=0; i<4; i++)
-            on_data[i] = data[i];
-    }
-    else
-    {
-        for(i=0; i<4; i++)
-            off_data[i] = data[i];
-    }
-
-    for(i=0; i<4; i++)
-    {
-        if(on_data[i] > off_data[i])
-            channel_data[i] = on_data[i] - off_data[i];
-        else
-			printk("ges on off error\n");
-    }
-
-    apply_filter(channel_data);
-
-
-    for(i=0; i<4; i++)
-    {
-        ges_raws[idx+i] = channel_data[i];
-
-        if(ges_count>=BUF_SIZE)
-        {
-            start_idx++;
-            if(start_idx>=BUF_SIZE)
-                start_idx=0;
-        }
-    }
-
-    ges_count+=4;
-    if(ges_count>=BUF_SIZE)
-        ges_count=BUF_SIZE;
-}

-/*
-//====================I2C read operation===============//
-*/
-static int elan_epl2182_I2C_Read(struct i2c_client *client)
-{
-    uint8_t buffer[RXBYTES];
-    int ret = 0, i =0;
-    int retry;
-
-    for(retry = 0; retry < I2C_RETRY_COUNT; retry++)
-    {
-        ret = i2c_master_recv(client, buffer, RXBYTES);
-
-        if (ret == RXBYTES)
-            break;

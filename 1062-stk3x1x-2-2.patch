+	queue_work(obj->stk_ps_tune0_wq, &obj->stk_ps_tune0_work);	
+	hrtimer_forward_now(&obj->ps_tune0_timer, obj->ps_tune0_delay);
+	return HRTIMER_RESTART;	
+}
